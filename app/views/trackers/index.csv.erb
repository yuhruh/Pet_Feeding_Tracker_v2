<%= fields = [:Date, :Time, :Food_Type, :Brand, :Description, :Amount, :Total_Ate_Amount, :Weight, :Note].join(';') %>
<% @trackers.each do |tracker| %>
<% date = tracker.date.strftime("%Y-%m-%d") %>
<% time = tracker.feeding_time.strftime("%I:%M%p") %>
<% food_type = tracker.food_type %>
<% brand = tracker.brand %>
<% description = tracker.description %>
<% amount = tracker.amount %>
<% total_ate_amount = tracker.total_ate_amount %>
<% weight = tracker.weight %>
<% note = tracker.note %>
<%= [date, time, food_type, brand, description, amount, total_ate_amount, weight, note].join(';') %>
<% end %>